{
  "name": "coffee-shop-admin",
  "version": "1.5.1",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "audit:prod": "npm audit --production --audit-level=high",
    "audit:report": "npm audit --json > ../reports/npm-audit-report.json",
    "type-check": "vue-tsc --noEmit",
    "electron:dev": "vite",
    "electron:build": "cross-env ELECTRON=true vite build && electron-builder",
    "electron:build:win": "cross-env ELECTRON=true CSC_IDENTITY_AUTO_DISCOVERY=false vite build && electron-builder --win",
    "electron:build:portable": "cross-env ELECTRON=true CSC_IDENTITY_AUTO_DISCOVERY=false vite build && electron-builder --win portable",
    "electron:build:portable:clean": "rimraf dist dist-electron release && cross-env ELECTRON=true CSC_IDENTITY_AUTO_DISCOVERY=false vite build && electron-builder --win portable",
    "electron:preview": "electron ."
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/forms": "^0.5.9",
    "@vueuse/core": "^11.2.0",
    "axios": "^1.7.7",
    "chart.js": "^4.4.6",
    "date-fns": "^4.1.0",
    "pinia": "^2.1.7",
    "vue": "^3.5.12",
    "vue-chartjs": "^5.3.2",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.1.0",
    "electron": "^37.0.0",
    "electron-builder": "^25.1.7",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "happy-dom": "^20.0.10",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^6.0.0",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.9",
    "vue-tsc": "^2.1.10",
    "workbox-window": "^7.3.0"
  }
}

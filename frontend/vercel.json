{
    "rewrites": [
      { 
        "source": "/((?!assets|icons|favicon\\.png|manifest\\.json).*)", 
        "destination": "/index.html" 
      }
    ],
    "headers": [
      {
        "source": "/assets/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
}